@model IList<RegisteredUserReportLineModel>

<div class="widget widget-table">

    <div class="widget-header">
        <i class="icon-group"></i>
        <h3>@T("Admin.Users.Reports.RegisteredUsers")</h3>
    </div>

    <div class="widget-content">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>@T("Admin.Users.Reports.RegisteredUsers.Fields.Period")</th>
                    <th>@T("Admin.Users.Reports.RegisteredUsers.Fields.Users")</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var x in Model)
                {
                    <tr>
                        <td>@x.Period</td>
                        <td>@x.Users</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>